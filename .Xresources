! URXVT Terminal
! Set depth to make transparency work.
URxvt*depth: 32
URxvt*scrollBar:         fase
URxvt*scrollBar_right:   false

URxvt*transparent:       false

URxvt.font:xft:SauceCodePro Nerd Font Mono:pixelsize=20
URxvt.letterSpace: 0

*antialias:  true
*autohint:   true

!yellow box at the bottom
URxvt.iso14755:    false
URxvt.iso14755_52: false

!EXTENSIONS
URxvt.perl-ext-common: default,font-size
URxvt.Keysym.Shift-Control-V:     eval:paste_clipboard
URxvt.Keysym.Shift-Control-C:     eval:selection_to_clipboard

!disable previous keybinds for copying and pasting? Stackoverflow knows best
URxvt.Keysym.Control-Meta-c: builtin-string:
URxvt.Keysym.Control-Meta-v: builtin-string:

URxvt.Keysym.C-k:                 font-size:increase
URxvt.Keysym.C-j:                 font-size:decrease

